{
  "name": "react-app-ci-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "http-server": "^0.10.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "serve": "^6.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": ["prettier --single-quote --write", "git add"]
  },
  "scripts": {
    "start": "react-scripts start",
    "serve": "http-server ./build -p 8081",
    "build": "react-scripts build",
    "test-local": "react-scripts test --env=jsdom --watchAll",
    "test": "react-scripts test --env=jsdom",
    "cypress": "cypress run --record",
    "cypress-local": "cypress open",
    "eject": "react-scripts eject",
    "lint": "eslint src/*.js",
    "analyze": "source-map-explorer build/static/js/main.*",
    "precommit": "lint-staged",
    "prepush": "yarn test-local",
    "serve-test": "serve -s build -p 3000 & yarn run cypress"
  },
  "devDependencies": {
    "cypress": "^1.0.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.14.3",
    "jest": "^21.1.0",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.4",
    "react-scripts": "^1.0.14",
    "react-test-renderer": "^15.6.1",
    "source-map-explorer": "^1.5.0",
    "stylelint-config-standard": "^17.0.0"
  }
}
